{
	"name": "anahkiasen/rocketeer",
	"description": "Rocketeer provides a fast and easy way to deploy your Laravel projects",
	"license": "MIT",
	"keywords": [
		"laravel",
		"deployment",
		"ssh"
	],
	"authors": [
		{
			"name": "Maxime Fabre",
			"email": "ehtnam6@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.0",
		"illuminate/support": "~4",
		"illuminate/config": "~4",
		"illuminate/console": "~4",
		"illuminate/container": "~4",
		"illuminate/filesystem": "~4",
		"illuminate/events": "~4"
	},
	"require-dev": {
		"mockery/mockery": "dev-master",
		"nesbot/carbon": "dev-master",
		"patchwork/utf8": "dev-master",
		"herrera-io/box": "dev-master",
		"illuminate/remote": "~4",
		"phpseclib/phpseclib": "dev-master"
	},
	"suggest": {
		"anahkiasen/rocketeer-campfire": "Campfire plugin to create deployments notifications"
	},
	"minimum-stability": "dev",
	"bin": [
		"bin/rocketeer"
	],
	"autoload": {
		"classmap": ["tests"],
		"psr-0": {
			"Rocketeer": "src"
		}
	}
}
